<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="springUserAction" class="framgiavn.project01.web.action.UserAction"
		scope="prototype">
		<property name="userBusiness" ref="UserBusiness" />
	</bean>

	<bean id="springAdminUserAction" class="framgiavn.project01.web.action.admin.UserAction"
		scope="prototype">
		<property name="userBusiness" ref="UserBusiness" />
	</bean>

	<bean id="springCategoryAction" class="framgiavn.project01.web.action.CategoryAction"
		scope="prototype">
		<property name="categoryBusiness" ref="CategoryBusiness" />
	</bean>

	<bean id="springAdminCategoryAction" class="framgiavn.project01.web.action.admin.CategoryAction"
		scope="prototype">
		<property name="categoryBusiness" ref="CategoryBusiness" />
	</bean>

	<bean id="springSessionAction" class="framgiavn.project01.web.action.SessionAction"
		scope="prototype">
		<property name="userBusiness" ref="UserBusiness" />
	</bean>
</beans>