<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!-- Configure NameSpace for action web -->
	<package name="Login" namespace="/" extends="default-web">
		<action name="login" class="springSessionAction" method="login">
			<result name="success">User.Info</result>
			<result name="error">Login</result>
		</action>
	</package>
</struts>